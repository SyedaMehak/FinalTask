<%
if(!locals.users){
    locals.users = [];
}
%>
    <html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="./assets/css/dashboard-styles.css">
        <!-- Latest compiled and minified CSS -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">

        <!-- Optional theme -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css">
        <link rel="stylesheet" href="./assets/css/style.css">
        <!-- Latest compiled and minified JavaScript -->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>


        <title>dashboard</title>
    </head>

    <body style="background-color: #d3ffff">
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="main-box clearfix">
                        <div class="table-responsive">
                            <table class="table user-list">
                                <thead>
                                    <tr>

                                        <th><span>Name</span></th>
                                        <th><span>Date</span></th>
                                        <th><span>Email</span></th>
                                        <th>Modify Data&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(let user of users) {%>
                                        <tr>

                                            <td>
                                                <%=user.name%>
                                            </td>


                                            <td>
                                                <%=user.date%>
                                            </td>
                                            <td>
                                                <a href="#">
                                                    <%=user.email%>
                                                </a>
                                            </td>
                                            <td style="width: 20%;">
                                                <a href="/addUser" class="table-link">
                                                    <span class="fa-stack">
										<i class="fa fa-square fa-stack-2x"></i>
										<i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
									</span>
                                                </a>
                                                <a href="update/<%=user.id%>" class="table-link">
                                                    <span class="fa-stack">
										<i class="fa fa-square fa-stack-2x"></i>
										<i class="fa fa-refresh fa-stack-1x fa-inverse"></i>
									</span>
                                                </a>
                                                <a href="delete/<%=user.id%>" class="table-link danger">
                                                    <span class="fa-stack">
										<i class="fa fa-square fa-stack-2x"></i>
										<i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
									</span>
                                                </a>
                                            </td>
                                        </tr>
                                        <% } %>
                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <a class="buttonAdd" href="/addUser"> Add User</a>
    </body>

    </html>